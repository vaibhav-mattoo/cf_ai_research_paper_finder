{
  "name": "cf_ai_research_paper_finder",
  "version": "1.0.0",
  "description": "AI-powered research paper finder using Cloudflare Agents",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "pages:dev": "wrangler pages dev ./frontend",
    "pages:deploy": "wrangler pages deploy ./frontend",
    "deploy:all": "npm run deploy && npm run pages:deploy",
    "test": "node test.js",
    "test:unit": "node --experimental-modules src/tests/test-suite.js",
    "setup": "npm install && wrangler login",
    "lint": "echo 'Linting not configured yet'",
    "build": "echo 'Build not required for Workers'"
  },
  "dependencies": {
    "agents": "^0.1.0"
  },
  "devDependencies": {
    "wrangler": "^4.40.1"
  },
  "keywords": [
    "ai",
    "research",
    "papers",
    "cloudflare",
    "agents"
  ],
  "author": "AI Research Paper Finder",
  "license": "MIT"
}
